<tr id='<%= dom_id(slider) %>'>
  <td>
    <%= image_tag(slider.image_attached, size:"50x50") %>
    <%= slider.name %>
  </td>
  <td class='text-right'>
    <%= link_to I18n.t(:edit), edit_backend_slider_path(slider), remote: true, class: 'btn btn-sm btn-outline-warning' %>
    <%= link_to I18n.t(:destroy), backend_slider_path(slider), method: :delete, remote: true, data: { confirm: 'Esta seguro?' }, class: 'btn btn-sm btn-outline-danger' %>
  </td>
</tr>
